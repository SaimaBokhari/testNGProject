<!-- Run xyzBankTest(), totalPrice() and createPatient() methods as a group test  -->
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Practice" verbose="1">
    <test name="Practice Smoke Test">
        <groups>
            <run>
                <include name="smoke-test" />
            </run>
        </groups>

        <packages>
            <!-- .* means ALL PACKAGES -->
            <package name="techproed.tests.*"/>
        </packages>

    </test>


</suite>
